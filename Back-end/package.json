{
  "name": "ts-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/server.js",
    "watch": "nodemon ./dist/bin/server.js",
    "watchDev": "nodemon -e ts,tsx --exec ts-node ./bin/server.ts",
    "build-ts": "tsc",
    "copy-static": "ts-node copyStatic.ts",
    "build": "npm run build-ts && npm run copy-static"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "excel-export": "^0.5.1",
    "express": "~4.16.1",
    "express-jwt": "^6.0.0",
    "http-errors": "~1.6.3",
    "install": "^0.13.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.0.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/express-jwt": "^6.0.1",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.3.0",
    "@types/morgan": "^1.9.2",
    "@types/mysql": "^2.15.16",
    "@types/node": "^14.14.16",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.6",
    "shelljs": "^0.8.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
